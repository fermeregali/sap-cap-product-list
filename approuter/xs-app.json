{
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/products/(.*)$",
      "target": "/$1",
      "destination": "products-destination",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)$",
      "localDir": "resources",
      "authenticationType": "none"
    }
  ]
}
